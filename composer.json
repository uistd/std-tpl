{
  "name": "uistd/tpl",
  "description": "UI Service 轻量化模板组件，兼容大部分Smarty语法，但不会生成临时文件，生成的代码会缓存在内存中（如果加载了apcu扩展）",
  "keywords": [
    "ui service",
    "uistd",
    "tpl"
  ],
  "type": "composer",
  "authors": [
    {
      "name": "huangshunzhao",
      "email": "huangshunzhao@wanda.cn",
      "role": "Founder and project lead"
    }
  ],
  "require": {
    "php": ">=5.5.0",
    "uistd/common": "3.*"
  },
  "autoload": {
    "psr-4": {
      "UiStd\\Tpl\\": "src"
    }
  },
  "minimum-stability": "stable",
  "archive": {
    "exclude": [
      "test",
      ".gitignore"
    ]
  }
}